<h1>New Startup</h1>

<%= form_for @startup, :url => url_for(:controller => 'startups', :action => 'create') do |f| %>
<%= f.label :title %>
<%= f.text_field :title, class: 'block-style' %>
<%= f.label :author %>
<%= f.text_field :author, class: 'block-style' %>
<%= f.label :description %>
<%= f.text_field :description, class: 'block-style' %>
<%= f.label :category %>
<%= f.text_field :category, class: 'block-style' %>
<%= f.label :img_url %>
<%= f.text_field :img, class: 'block-style' %>
<%= f.label :body %>
<%= f.text_field :body, class: 'block-style block-style-last' %>
<%= f.submit %>
<% end %>

<%= link_to 'Back', startups_path %>
